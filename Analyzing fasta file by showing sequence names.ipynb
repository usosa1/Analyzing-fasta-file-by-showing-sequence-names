    "def pick_seq_name(file_path):\n",
    "    f=open(file_path)\n",
    "    lines = f.readlines()\n",
    "    for line in lines:\n",
    "       if line[0] == \">\":\n",
    "          seqName = line[:-1]\n",
    "          print(seqName)\n",
    "\n",
    "print(pick_seq_name('MultiSeq.Afasta'))\n"
